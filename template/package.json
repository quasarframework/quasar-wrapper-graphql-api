{
  "name": "apollo-starter-kit",
  "version": "0.1.0",
  "description": "Minimal set of code to write a GraphQL server with Apollo graphql-tools",
  "scripts": {
    "start": "nodemon ./server.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quasarframework/quasar-wrapper-graphql-api"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "GraphQL",
    "Express",
    "Apollo",
    "Quasar"
  ],
  "author": "Scott Molinari <scott.molinari@adduco.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quasarframework/quasar"
  },
  "homepage": "http://quasar-framework.org",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "4.13.4",
    "graphql-server-express": "^0.6.0",
    "graphql-tools": "^0.11.0"
  },
  "devDependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "^2.4.0",
    "eslint-plugin-import": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "graphql": "^0.9.2",
    "nodemon": "^1.9.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
    ],
    "rules": {
      "no-use-before-define": 0,
      "arrow-body-style": 0,
      "dot-notation": 0,
      "no-console": 0
    },
    "env": {
      "mocha": true
    }
  }
}
